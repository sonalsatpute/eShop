{
  "SERVICE_NAME": "eShop.WebApi",
  "open_telemetry_collector_url": "http://localhost:4317",
  "open_telemetry_tracing_enabled": true,
  "open_telemetry_metrics_enabled": true,
  "open_telemetry_logging_enabled": true,
  "open_telemetry_console_exporter_enabled": true,
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=../eShop.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
